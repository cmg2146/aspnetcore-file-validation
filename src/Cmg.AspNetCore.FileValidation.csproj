<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Description>ASP.NET Core validation attributes to validate file uploads.</Description>
    <PackageId>Cmg.AspNetCore.FileValidation</PackageId>
    <PackageTags>aspnetcore;validation;files;uploads</PackageTags>
    <Version>1.0.0</Version>
    <Authors>Clark Granum</Authors>
    <Company>CMG</Company>
    <RepositoryUrl>https://github.com/cmg2146/aspnetcore-file-validation</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/cmg2146/aspnetcore-file-validation</PackageProjectUrl>
    <IsTrimmable>true</IsTrimmable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
</Project>
